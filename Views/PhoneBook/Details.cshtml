<!-- Details.cshtml -->
@model Contact

<h2>Details</h2>

<table>
    <tr>
        <td>ID:</td>
        <td>@Model.ID</td>
    </tr>
    <tr>
        <td>Last Name:</td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>First Name:</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Middle Name:</td>
        <td>@Model.MiddleName</td>
    </tr>
    <tr>
        <td>Phone Number:</td>
        <td>@Model.PhoneNumber</td>
    </tr>
    <tr>
        <td>Address:</td>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <td>Description:</td>
        <td>@Model.Description</td>
    </tr>
</table>